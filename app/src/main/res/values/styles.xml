<resources>

    <!-- Base application theme. -->
    <style name="TemaZaApp" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="HeaderBar">
        <item name="android:background">@color/colorPrimary</item>
    </style>

    <style name="TemaZaApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TemaZaApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="TemaZaApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
